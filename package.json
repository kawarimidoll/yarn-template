    "*.{js,jsx,vue,json,md,html,css,yml}": "prettier --write"
  },
  "devDependencies": {
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^7.0.2",
    "lint-staged": "^11.2.3",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "sort-package-json": "^1.52.0"
  }
}
