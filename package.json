{
  "name": "yarn-template",
  "version": "0.0.0",
  "repository": "https://github.com/kawarimidoll/yarn-template",
  "license": "MIT",
  "author": "kawarimidoll <kawarimidoll+git@gmail.com>",
  "main": "index.js",
  "scripts": {
    "preci": "rm -rf node_modules",
    "ci": "yarn install --frozen-lockfile",
